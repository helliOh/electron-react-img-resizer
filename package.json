{
  "name": "resizer-beta",
  "version": "1.1.0",
  "private": true,
  "main": "electron/index.js",
  "homepage": "./",
  "dependencies": {
    "@ant-design/icons": "^4.6.3",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "antd": "^4.16.13",
    "buffer": "^6.0.3",
    "buffer-image-size": "^0.6.4",
    "cross-env": "^7.0.3",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "sharp": "^0.29.0",
    "smartcrop": "^2.0.4",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "cross-env BROWSER=none react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "app:react": "yarn start",
    "preapp:electron": "electron-rebuild",
    "app:electron": "cross-env ELECTRON_START_URL=http://localhost:3000 electron .",
    "app:electron:standalone": "electron .",
    "app:react:build": "react-scripts build",
    "app:electron:build": "electron-builder build",
    "predeploy": "cp electron/index.js public/electron.js && cp electron/preload.js public/preload.js",
    "deploy": " yarn app:react:build && yarn app:electron:build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^13.2.2",
    "electron-builder": "^22.11.7",
    "electron-rebuild": "^3.2.0"
  }
}
